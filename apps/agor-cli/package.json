{
  "name": "@agor/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "agor": "./bin/run.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "NODE_NO_WARNINGS=1 tsx bin/dev.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agor/core": "workspace:*",
    "@oclif/core": "^4.6.0",
    "@oclif/plugin-help": "^6.2.18",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "inquirer": "^12.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "glob": "^11.1.0",
    "oclif": "^4.22.32",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "oclif": {
    "bin": "agor",
    "dirname": "agor",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "helpClass": "./dist/lib/help.js",
    "hooks": {
      "command_not_found": "./dist/hooks/command-not-found.js"
    }
  }
}
