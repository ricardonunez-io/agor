{
  "name": "agor-live",
  "version": "0.3.8",
  "description": "Multiplayer canvas for orchestrating AI coding sessions",
  "type": "module",
  "bin": {
    "agor": "./bin/agor.js",
    "agor-daemon": "./bin/agor-daemon.js"
  },
  "files": [
    "bin/",
    "dist/",
    "node_modules/@agor",
    "LICENSE",
    "README.md"
  ],
  "imports": {
    "@agor/core": "./dist/core/index.js",
    "@agor/core/*": "./dist/core/*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "codex",
    "gemini",
    "multiplayer",
    "collaboration",
    "git",
    "workflow"
  ],
  "author": "Maxime Beauchemin <maximebeauchemin@gmail.com>",
  "license": "BUSL-1.1",
  "homepage": "https://agor.live",
  "repository": {
    "type": "git",
    "url": "https://github.com/mistercrunch/agor.git"
  },
  "bugs": {
    "url": "https://github.com/mistercrunch/agor/issues"
  },
  "dependencies": {
    "@google/gemini-cli-core": "^0.9.0",
    "@google/genai": "^1.26.0",
    "@homebridge/node-pty-prebuilt-multiarch": "^0.13.1",
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@oclif/core": "^4.6.0",
    "@oclif/plugin-help": "^6.2.18",
    "@openai/codex-sdk": "^0.47.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "simple-git": "^3.28.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "uuidv7": "^1.0.2"
  },
  "oclif": {
    "bin": "agor",
    "dirname": "agor",
    "commands": "./dist/cli/commands",
    "topicSeparator": " ",
    "helpClass": "./dist/cli/lib/help.js",
    "hooks": {
      "command_not_found": "./dist/cli/hooks/command-not-found.js"
    }
  }
}
