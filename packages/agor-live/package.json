{
  "name": "agor-live",
  "version": "0.11.2",
  "description": "Multiplayer canvas for orchestrating AI coding sessions",
  "type": "module",
  "bin": {
    "agor": "./bin/agor.js",
    "agor-daemon": "./bin/agor-daemon.js"
  },
  "files": [
    "bin/",
    "dist/",
    "scripts/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js"
  },
  "imports": {
    "@agor/core": "./dist/core/index.js",
    "@agor/core/*": "./dist/core/*"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "codex",
    "gemini",
    "multiplayer",
    "collaboration",
    "git",
    "workflow"
  ],
  "author": "Maxime Beauchemin <maximebeauchemin@gmail.com>",
  "license": "BUSL-1.1",
  "homepage": "https://agor.live",
  "repository": {
    "type": "git",
    "url": "https://github.com/preset-io/agor.git"
  },
  "bugs": {
    "url": "https://github.com/preset-io/agor/issues"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.33",
    "@feathersjs/authentication": "^5.0.37",
    "@feathersjs/authentication-client": "^5.0.37",
    "@feathersjs/authentication-local": "^5.0.37",
    "@feathersjs/errors": "^5.0.37",
    "@feathersjs/express": "^5.0.37",
    "@feathersjs/feathers": "^5.0.37",
    "@feathersjs/rest-client": "^5.0.37",
    "@feathersjs/schema": "^5.0.37",
    "@feathersjs/socketio": "^5.0.37",
    "@feathersjs/socketio-client": "^5.0.37",
    "@feathersjs/typebox": "^5.0.37",
    "@google/gemini-cli-core": "^0.27.2",
    "@google/genai": "^1.40.0",
    "@homebridge/node-pty-prebuilt-multiarch": "^0.13.1",
    "@iarna/toml": "^2.2.5",
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@oclif/core": "^4.6.0",
    "@oclif/plugin-help": "^6.2.18",
    "@openai/codex-sdk": "^0.98.0",
    "@opencode-ai/sdk": "^1.1.53",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cron-parser": "^5.4.0",
    "cronstrue": "^3.9.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^5.2.0",
    "express-static-gzip": "^3.0.0",
    "feathers-swagger": "^3.0.0",
    "handlebars": "^4.7.8",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "open": "^11.0.0",
    "postgres": "^3.4.7",
    "simple-git": "^3.30.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-dist": "^5.30.0",
    "uuid": "^13.0.0",
    "uuidv7": "^1.0.2",
    "zod": "^3.25.0"
  },
  "oclif": {
    "bin": "agor",
    "dirname": "agor",
    "commands": "./dist/cli/commands",
    "topicSeparator": " ",
    "helpClass": "./dist/cli/lib/help.js",
    "hooks": {
      "command_not_found": "./dist/cli/hooks/command-not-found.js"
    }
  }
}
